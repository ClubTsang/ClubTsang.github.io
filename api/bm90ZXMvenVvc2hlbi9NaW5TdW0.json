\"hljs-keyword\">int</span> j = i + <span class=\"hljs-number\">1</span>; j &lt; n; ++j)<br>        &#123;<br>            <span class=\"hljs-keyword\">if</span> (A[i] &lt; A[j])<br>            &#123;<br>                res += A[i];<br>            &#125;<br>        &#125;<br>    &#125;<br>    <span class=\"hljs-keyword\">return</span> res;<br>&#125;<br><br><span class=\"hljs-comment\">//小和问题 ---&gt; 归并排序思想解决</span><br><span class=\"hljs-function\"><span class=\"hljs-keyword\">int</span> <span class=\"hljs-title\">getSmallSum_2</span><span class=\"hljs-params\">(<span class=\"hljs-keyword\">int</span> A[], <span class=\"hljs-keyword\">int</span> l, <span class=\"hljs-keyword\">int</span> r)</span></span><br><span class=\"hljs-function\"></span>&#123;<br>    <span class=\"hljs-keyword\">if</span> (<span class=\"hljs-literal\">NULL</span> == A || l &gt;= r)<br>    &#123;<br>        <span class=\"hljs-keyword\">return</span> <span class=\"hljs-number\">0</span>;<br>    &#125;<br><br>    <span class=\"hljs-keyword\">int</span> mid = l + (r - l) / <span class=\"hljs-number\">2</span>;<br>    <span class=\"hljs-keyword\">int</span> res = <span class=\"hljs-built_in\">getSmallSum_2</span>(A, l, mid) + <span class=\"hljs-built_in\">getSmallSum_2</span>(A, mid + <span class=\"hljs-number\">1</span>, r);<br><br>    <span class=\"hljs-comment\">//归并</span><br>    <span class=\"hljs-keyword\">int</span> p1 = l, p2 = mid + <span class=\"hljs-number\">1</span>, i = <span class=\"hljs-number\">0</span>;<br>    <span class=\"hljs-keyword\">int</span> *help = <span class=\"hljs-keyword\">new</span> <span class=\"hljs-keyword\">int</span>[r - l + <span class=\"hljs-number\">1</span>];<br>    <span class=\"hljs-keyword\">while</span> (p1 &lt;= mid &amp;&amp; p2 &lt;= r)<br>    &#123;<br>        res += (A[p1] &lt; A[p2] ? (r - p2 + <span class=\"hljs-number\">1</span>) * A[p1] : <span class=\"hljs-number\">0</span>);<br>        help[i++] = A[p1] &lt; A[p2] ? A[p1++] : A[p2++];<br>    &#125;<br>    <span class=\"hljs-keyword\">while</span> (p1 &lt;= mid)<br>    &#123;<br>        help[i++] = A[p1++];<br>    &#125;<br>    <span class=\"hljs-keyword\">while</span> (p2 &lt;= r)<br>    &#123;<br>        help[i++] = A[p2++];<br>    &#125;<br>    <span class=\"hljs-keyword\">for</span> (i = <span class=\"hljs-number\">0</span>; i &lt; r - l + <span class=\"hljs-number\">1</span>; ++i)<br>    &#123;<br>        A[l + i] = help[i];<br>    &#125;<br>    <span class=\"hljs-keyword\">delete</span> help;<br>    help = <span class=\"hljs-literal\">NULL</span>;<br>    <span class=\"hljs-keyword\">return</span> res;<br>&#125;<br><br><span class=\"hljs-comment\">//打印数组函数</span><br><span class=\"hljs-function\"><span class=\"hljs-keyword\">void</span> <span class=\"hljs-title\">MyPrintfArray</span><span class=\"hljs-params\">(<span class=\"hljs-keyword\">int</span> A[], <span class=\"hljs-keyword\">int</span> n)</span></span>;<br><span class=\"hljs-comment\">//判断两个数组是否相等</span><br><span class=\"hljs-function\"><span class=\"hljs-keyword\">bool</span> <span class=\"hljs-title\">ArrayIsEqual</span><span class=\"hljs-params\">(<span class=\"hljs-keyword\">int</span> A[], <span class=\"hljs-keyword\">int</span> B[], <span class=\"hljs-keyword\">int</span> n)</span></span>;<br><span class=\"hljs-comment\">//测试函数</span><br><span class=\"hljs-function\"><span class=\"hljs-keyword\">void</span> <span class=\"hljs-title\">test</span><span class=\"hljs-params\">()</span></span>;<br><br><span class=\"hljs-function\"><span class=\"hljs-keyword\">int</span> <span class=\"hljs-title\">main</span><span class=\"hljs-params\">()</span></span><br><span class=\"hljs-function\"></span>&#123;<br>    <span class=\"hljs-built_in\">test</span>();<br>    <span class=\"hljs-keyword\">return</span> <span class=\"hljs-number\">0</span>;<br>&#125;<br><br><span class=\"hljs-function\"><span class=\"hljs-keyword\">void</span> <span class=\"hljs-title\">test</span><span class=\"hljs-params\">()</span></span><br><span class=\"hljs-function\"></span>&#123;<br>    <span class=\"hljs-keyword\">int</span> A[<span class=\"hljs-number\">20</span>], B[<span class=\"hljs-number\">20</span>], C[<span class=\"hljs-number\">20</span>];<br>    <span class=\"hljs-keyword\">int</span> count = <span class=\"hljs-number\">0</span>;<br>    <span class=\"hljs-keyword\">int</span> n;<br>    <span class=\"hljs-keyword\">bool</span> b = <span class=\"hljs-literal\">true</span>;<br>    <span class=\"hljs-built_in\">srand</span>((<span class=\"hljs-keyword\">unsigned</span>)<span class=\"hljs-built_in\">time</span>(<span class=\"hljs-literal\">NULL</span>));<br>    <span class=\"hljs-keyword\">int</span> res1 = <span class=\"hljs-number\">0</span>, res2 = <span class=\"hljs-number\">0</span>;<br>    <span class=\"hljs-keyword\">while</span> (count &lt; <span class=\"hljs-number\">500000</span> &amp;&amp; b)<br>    &#123;<br>        ++count;<br>        n = <span class=\"hljs-built_in\">rand</span>() % <span class=\"hljs-number\">5</span> + <span class=\"hljs-number\">10</span>;<br>        <span class=\"hljs-keyword\">int</span> RandomNumber;<br>        <span class=\"hljs-keyword\">for</span> (<span class=\"hljs-keyword\">int</span> i = <span class=\"hljs-number\">0</span>; i &lt; n; ++i)<br>        &#123;<br>            RandomNumber = <span class=\"hljs-built_in\">rand</span>() % <span class=\"hljs-number\">100</span> + <span class=\"hljs-number\">1</span>;<br>            A[i] = B[i] = C[i] = RandomNumber;<br>        &#125;<br><br>        res1 = <span class=\"hljs-built_in\">getSmallSum_1</span>(B, n);<br>        <span class=\"hljs-built_in\">sort</span>(B, B + n);<br>        res2 = <span class=\"hljs-built_in\">getSmallSum_2</span>(C, <span class=\"hljs-number\">0</span>, n - <span class=\"hljs-number\">1</span>);<br>        b = <span class=\"hljs-built_in\">ArrayIsEqual</span>(B, C, n) &amp;&amp; res1 == res2;<br>    &#125;<br>    <span class=\"hljs-keyword\">if</span> (b)<br>    &#123;<br>        cout &lt;&lt; <span class=\"hljs-string\">&quot;Success&quot;</span> &lt;&lt; endl;<br>    &#125;<br>    <span class=\"hljs-keyword\">else</span><br>    &#123;<br>        cout &lt;&lt; <span class=\"hljs-string\">&quot;Unsuccess&quot;</span> &lt;&lt; endl;<br>        <span class=\"hljs-built_in\">MyPrintfArray</span>(A, n);<br>        <span class=\"hljs-built_in\">MyPrintfArray</span>(B, n);<br>        <span class=\"hljs-built_in\">MyPrintfArray</span>(C, n);<br>        cout &lt;&lt; <span class=\"hljs-string\">&quot;[res1:&quot;</span> &lt;&lt; res1 &lt;&lt; <span class=\"hljs-string\">&quot;   res2:&quot;</span> &lt;&lt; res2 &lt;&lt; <span class=\"hljs-string\">&quot;]&quot;</span> &lt;&lt; endl;<br>    &#125;<br>&#125;<br><br><span class=\"hljs-comment\">//打印数组函数</span><br><span class=\"hljs-function\"><span class=\"hljs-keyword\">void</span> <span class=\"hljs-title\">MyPrintfArray</span><span class=\"hljs-params\">(<span class=\"hljs-keyword\">int</span> A[], <span class=\"hljs-keyword\">int</span> n)</span></span><br><span class=\"hljs-function\"></span>&#123;<br>    <span class=\"hljs-keyword\">if</span> (<span class=\"hljs-literal\">NULL</span> == A || n &lt; <span class=\"hljs-number\">1</span>)<br>    &#123;<br>        <span class=\"hljs-keyword\">return</span>;<br>    &#125;<br>    <span class=\"hljs-keyword\">for</span> (<span class=\"hljs-keyword\">int</span> i = <span class=\"hljs-number\">0</span>; i &lt; n; ++i)<br>    &#123;<br>        <span class=\"hljs-keyword\">if</span> (i == <span class=\"hljs-number\">0</span>)<br>        &#123;<br>            cout &lt;&lt; <span class=\"hljs-string\">&quot;[&quot;</span>;<br>        &#125;<br>        cout &lt;&lt; A[i];<br>        <span class=\"hljs-keyword\">if</span> (i == n - <span class=\"hljs-number\">1</span>)<br>        &#123;<br>            cout &lt;&lt; <span class=\"hljs-string\">&quot;]&quot;</span> &lt;&lt; endl;<br>        &#125;<br>        <span class=\"hljs-keyword\">else</span><br>        &#123;<br>            cout &lt;&lt; <span class=\"hljs-string\">&quot;,&quot;</span>;<br>        &#125;<br>    &#125;<br>&#125;<br><br><span class=\"hljs-comment\">//判断两个数组是否相等</span><br><span class=\"hljs-function\"><span class=\"hljs-keyword\">bool</span> <span class=\"hljs-title\">ArrayIsEqual</span><span class=\"hljs-params\">(<span class=\"hljs-keyword\">int</span> A[], <span class=\"hljs-keyword\">int</span> B[], <span class=\"hljs-keyword\">int</span> n)</span></span><br><span class=\"hljs-function\"></span>&#123;<br>    <span class=\"hljs-keyword\">if</span> (<span class=\"hljs-literal\">NULL</span> == A &amp;&amp; <span class=\"hljs-literal\">NULL</span> != B)<br>    &#123;<br>        <span class=\"hljs-keyword\">return</span> <span class=\"hljs-literal\">false</span>;<br>    &#125;<br>    <span class=\"hljs-keyword\">if</span> (<span class=\"hljs-literal\">NULL</span> == B &amp;&amp; <span class=\"hljs-literal\">NULL</span> != A)<br>    &#123;<br>        <span class=\"hljs-keyword\">return</span> <span class=\"hljs-literal\">false</span>;<br>    &#125;<br><br>    <span class=\"hljs-keyword\">for</span> (<span class=\"hljs-keyword\">int</span> i = <span class=\"hljs-number\">0</span>; i &lt; n; ++i)<br>    &#123;<br>        <span class=\"hljs-keyword\">if</span> (A[i] != B[i])<br>        &#123;<br>            <span class=\"hljs-keyword\">return</span> <span class=\"hljs-literal\">false</span>;<br>        &#125;<br>    &#125;<br>    <span class=\"hljs-keyword\">return</span> <span class=\"hljs-literal\">true</span>;<br>&#125;<br></code></pre></td></tr></table></figure>\n","link":"notes/zuoshen/MinSum","plink":"https://TOPUBLICZX.GITHUB.IO/notes/zuoshen/MinSum/","toc":[{"id":"1、description","title":"1、Description","index":"1"},{"id":"2、example","title":"2、Example","index":"2"},{"id":"3、code","title":"3、Code","index":"3"},{"id":"4、sumbit","title":"4、Sumbit","index":"4"}],"reward":true,"copyright":{"author":"CLUB TSANG","link":"<a href=\"https://TO